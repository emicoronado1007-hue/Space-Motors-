<!doctype html><html><head><meta charset='utf-8'><title>Admin</title><link rel='stylesheet' href='/public/css/styles.css'></head><body><%- include('../partials/header') %><div style='max-width:1100px;margin:0 auto;padding:16px'><h1>Panel</h1><p><a href="/admin/nuevo">Agregar auto</a></p><table border="0" cellpadding="8"><tr><th>Título</th><th>Precio</th><th>Año</th><th>KMs</th><th>Ciudad</th><th>Estado</th><th></th></tr><% cars.forEach(c=>{ %><tr><td><a href="/auto/<%= c.slug %>" target="_blank"><%= c.title %></a></td><td><%= formatMoney(c.price) %></td><td><%= c.year %></td><td><%= c.mileage.toLocaleString() %></td><td><%= c.city %></td><td><%= c.is_sold?'Vendido':'Disponible' %></td><td><a href="/admin/editar/<%= c.id %>">Editar</a></td></tr><% }) %></table></div></body></html>